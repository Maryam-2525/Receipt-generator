{
    "name": "receipt-generator",
    "version": "1.0.0",
    "homepage": "https://maryam-2525.github.io/Receipt-generator/",
    "scripts": {
        "start": "live-server src --port=3000",
        "build": "mkdir dist && xcopy /s /e /i src\\* dist",
        "predeploy": "npm run build",
        "deploy": "gh-pages --dist dist --branch gh-pages --message 'Deploy to GitHub Pages'"
    },
    "devDependencies": {
        "gh-pages": "^6.0.0",
        "live-server": "^1.2.2"
    }
}
